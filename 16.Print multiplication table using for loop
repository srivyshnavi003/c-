#include <iostream>

int main() {
    // Declare a variable to store the number for the multiplication table
    int number;

    // Read in a number
    std::cout << "Enter a number to display its multiplication table: ";
    std::cin >> number;

    // Display the multiplication table using a for loop
    std::cout << "Multiplication Table for " << number << ":\n";
    for (int i = 1; i <= 10; ++i) {
        std::cout << number << " * " << i << " = " << (number * i) << std::endl;
    }

    return 0;
}
