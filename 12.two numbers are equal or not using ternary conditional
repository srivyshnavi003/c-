#include <iostream>

int main() {
    // Declare variables to store two numbers
    int num1, num2;

    // Read in two numbers
    std::cout << "Enter the first number: ";
    std::cin >> num1;

    std::cout << "Enter the second number: ";
    std::cin >> num2;

    // Use the ternary conditional operator to check equality
    std::cout << "The numbers are " << (num1 == num2 ? "equal" : "not equal") << std::endl;

    return 0;
}
